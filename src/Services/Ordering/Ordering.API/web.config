<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!--
    Configure your application settings in appsettings.json. Learn more at http://go.microsoft.com/fwlink/?LinkId=786380
  -->
  <system.webServer>
    <handlers>
      <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
    </handlers>
    <aspNetCore processPath="%LAUNCHER_PATH%" arguments="%LAUNCHER_ARGS%" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" forwardWindowsAuthToken="false" hostingModel="InProcess">
      <environmentVariables>
        <environmentVariable name="COMPLUS_ForceENC" value="1" />
        <environmentVariable name="ASPNETCORE_ENVIRONMENT" value="Development" />
        <environmentVariable name="ConnectionString" value="Server=localhost;Initial Catalog=Microsoft.eShopOnContainers.Services.OrderingDb;Trusted_Connection=True;" />
        <environmentVariable name="RabbitMQPort" value="6672" />
        <environmentVariable name="IdentityUrlExternal" value="http://localhost:54010/" />
      </environmentVariables>
    </aspNetCore>
  </system.webServer>
</configuration>